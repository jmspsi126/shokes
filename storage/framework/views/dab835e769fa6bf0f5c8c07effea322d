  <script>
(function($) {
	$=jQuery;
  var rotate, timeline;

  rotate = function() {
    return $('.card:first-child').fadeOut(400, 'swing', function() {
      return $('.card:first-child').appendTo('.container_rotate').hide();
    }).fadeIn(400, 'swing');
  };

  timeline = setInterval(rotate, 1200);

  $('body').hover(function() {
    return clearInterval(timeline);
  });

  $('.card').click(function() {
	  //alert("fhdjfhjdhf");
    return rotate();
  });

}).call(this);
</script>
