    <script type="text/javascript" src="js/jquery.js"></script>
    <script type="text/javascript" src="js/bootstrap.min.js"></script>
    <script type="text/javascript" src="js/iscroll.js"></script>
    <script type="text/javascript" src="js/main.js"></script>
    <script type="text/javascript" src="js/slick.min.js"></script>  
    <script type="text/javascript">
    if (jQuery( window ).width() > 1024) {
        jQuery('#carousel-example-generic').on('DOMMouseScroll mousewheel MozMousePixelScroll', function ( event ) 
        {
            //e.preventDefault();
            var totalItem = jQuery('#carousel-example-generic .item').length;
            if( event.originalEvent.detail > 0 || event.originalEvent.wheelDelta < 0 ) 
            { 
                if(jQuery(this).find('.active').index()==totalItem-1 || jQuery(this).find('.active').index() > totalItem-1)
                {
                    jQuery(".isscroll_dis_wrap").show()
                }
                else
                {
                    jQuery(".isscroll_dis_wrap").hide()
                }
                jQuery(this).carousel('next');
                
            } 
            else 
            {
                
                if(jQuery(this).find('.active').index()==0)
                {
                    jQuery(".isscroll_dis_wrap").show()
                }
                else
                {
                    jQuery(".isscroll_dis_wrap").hide()
                }
                jQuery(this).carousel('prev');
                
            }
            return false;
        });
        window.addEventListener('wheel', function(e) {
            //e.preventDefault();
            var totalItem = jQuery('#carousel-example-generic .item').length;
            if (e.deltaY < 0) 
            {
                jQuery(".isscroll_dis_wrap").fadeOut();
                jQuery(".isscroll_dis_wrap").attr('drct','up');
            }
            if (e.deltaY > 0) 
            {
                jQuery(".isscroll_dis_wrap").attr('drct','down');
            }
            if (jQuery(this).find('.active').index()==0 && jQuery(".isscroll_dis_wrap").attr('drct')=='up')
            {
                jQuery(".isscroll_dis_wrap").show();
            }
            else if(jQuery(this).find('.active').index() > 0 && jQuery(this).find('.active').index() < totalItem-1)
            {
                jQuery(".isscroll_dis_wrap").hide();
            }
            else if(jQuery(this).find('.active').index() == totalItem-1 && jQuery(".isscroll_dis_wrap").attr('drct')=='down')
            {
                jQuery(".isscroll_dis_wrap").show();
            }
            else
            {
                jQuery(".isscroll_dis_wrap").hide();
            }
        });
}

        jQuery('#carousel-example-generic').carousel({wrap: false});

    </script>
    
